{
  "title": "POSTAdequaciesResponse",
  "$schema": "http://json-schema.org/draft-04/schema#",
  "description": "Response shape for POST /api/adequacies/",
  "type": "array",
  "items": {
    "properties": {
      "id": {
        "type": "integer"
      },
      "distance_to_closest_provider": {
        "description": "Distance (in miles) to the closest provider",
        "type": "number"
      },
      "time_to_closest_provider": {
        "description": "Time (in minutes) to the closest provider",
        "type": "integer"
      },
      "closest_providers_by_distance": {
        "type": "array",
        "items": {
          "type": "integer"
        }
      },
      "closest_providers_by_time": {
        "type": "array",
        "items": {
          "type": "integer"
        }
      }
    },
    "additionalProperties": false,
    "required": ["id", "distance_to_closest_provider", "time_to_closest_provider", "closest_providers_by_distance", "closest_providers_by_time"]
  },
  "additionalProperties": false
}
