{
  "name": "@bayesimpact/tds-frontend",
  "version": "1.0.0",
  "main": "dist/bundle.js",
  "repository": "git@github.com:bayesimpact/tds-frontend.git",
  "author": "Boris Cherny <boris@performancejs.com>",
  "license": "Apache-2.0",
  "scripts": {
    "build": "webpack",
    "lint": "tslint src/**/*",
    "pretest": "npm run build",
    "start": "webpack-dev-server",
    "start:docker": "webpack-dev-server --host 0.0.0.0",
    "tdd": "concurrently -k 'jest --watch' 'npm run start' 'npm run watch'",
    "test": "jest",
    "watch": "webpack -w"
  },
  "dependencies": {
    "babydux": "^2.0.1",
    "chart.js": "^2.7.1",
    "chart.piecelabel.js": "^0.8.2",
    "lodash": "^4.17.4",
    "mapbox-gl": "^0.41.0",
    "material-ui": "^0.19.4",
    "mui-icons": "^1.2.1",
    "papaparse": "^4.3.6",
    "react": "^16.1.0",
    "react-autocomplete": "^1.7.2",
    "react-chartjs-2": "^2.6.4",
    "react-dom": "^16.1.0",
    "react-mapbox-gl": "^2.7.0",
    "react-router-dom": "^4.2.2",
    "rx": "^4.1.0"
  },
  "devDependencies": {
    "@types/chart.js": "^2.6.12",
    "@types/jest": "^21.1.5",
    "@types/lodash": "^4.14.84",
    "@types/mapbox-gl": "^0.40.1",
    "@types/material-ui": "^0.18.4",
    "@types/papaparse": "^4.1.31",
    "@types/prop-types": "^15.5.2",
    "@types/react": "^16.0.22",
    "@types/react-dom": "^16.0.3",
    "@types/react-router-dom": "^4.2.1",
    "awesome-typescript-loader": "^3.3.0",
    "concurrently": "^3.5.0",
    "css-loader": "^0.28.7",
    "dotenv-webpack": "^1.5.4",
    "extract-text-webpack-plugin": "^3.0.2",
    "fetch-mock": "^5.13.1",
    "html-webpack-plugin": "^2.30.1",
    "jest": "^21.2.1",
    "jest-css-modules": "^1.1.0",
    "postcss-cssnext": "^3.0.2",
    "postcss-import": "^11.0.0",
    "postcss-loader": "^2.0.8",
    "source-map-loader": "^0.2.3",
    "ts-jest": "^21.2.1",
    "tslint": "^5.8.0",
    "tslint-react": "^3.2.0",
    "typescript": "^2.6.1",
    "webpack": "^3.8.1",
    "webpack-dev-server": "^2.9.4"
  },
  "jest": {
    "globals": {
      "ts-jest": {
        "skipBabel": true
      }
    },
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json"
    ],
    "moduleNameMapper": {
      "\\.(css)$": "<rootDir>/node_modules/jest-css-modules"
    },
    "setupFiles": [
      "./test/setupFiles.js"
    ],
    "transform": {
      "^.+\\.tsx?$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(\\.|/)test\\.(tsx?)$"
  }
}
